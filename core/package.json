{
  "name": "@dotdo/gitx",
  "version": "0.1.1",
  "description": "Pure Git implementation in TypeScript - object parsing, pack files, delta compression, refs. Zero dependencies.",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts"
    },
    "./objects": {
      "types": "./objects/index.ts",
      "import": "./objects/index.ts"
    },
    "./pack": {
      "types": "./pack/index.ts",
      "import": "./pack/index.ts"
    },
    "./refs": {
      "types": "./refs/index.ts",
      "import": "./refs/index.ts"
    },
    "./protocol": {
      "types": "./protocol/index.ts",
      "import": "./protocol/index.ts"
    }
  },
  "files": [
    "*.ts",
    "objects",
    "pack",
    "refs",
    "protocol"
  ],
  "keywords": [
    "git",
    "version-control",
    "vcs",
    "pack",
    "delta",
    "objects",
    "refs",
    "blob",
    "tree",
    "commit",
    "tag",
    "typescript",
    "pure",
    "portable"
  ],
  "author": "workers.do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/gitx.git",
    "directory": "core"
  }
}
