{
  "name": "@dotdo/gitx",
  "version": "0.1.0",
  "description": "Pure JavaScript Git implementation - platform agnostic core library",
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts"
    },
    "./objects": {
      "types": "./objects.ts",
      "import": "./objects.ts"
    },
    "./pack": {
      "types": "./pack.ts",
      "import": "./pack.ts"
    },
    "./refs": {
      "types": "./refs.ts",
      "import": "./refs.ts"
    },
    "./protocol": {
      "types": "./protocol.ts",
      "import": "./protocol.ts"
    }
  },
  "keywords": [
    "git",
    "pure",
    "javascript",
    "platform-agnostic",
    "no-dependencies"
  ],
  "author": "dot-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/gitx",
    "directory": "src/core"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
