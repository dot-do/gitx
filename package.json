{
  "name": "gitx.do",
  "version": "0.1.1",
  "description": "Git on Cloudflare Durable Objects - A complete git reimplementation for the edge",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gitx": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./do": {
      "types": "./dist/do/index.d.ts",
      "import": "./dist/do/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:node": "vitest run --config vitest.node.config.ts",
    "test:all": "vitest run && vitest run --config vitest.node.config.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "lint": "eslint src test --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "git",
    "cloudflare",
    "durable-objects",
    "edge",
    "mcp",
    "ai"
  ],
  "author": "dot-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/gitx"
  },
  "homepage": "https://github.com/dot-do/gitx#readme",
  "bugs": {
    "url": "https://github.com/dot-do/gitx/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.1",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^22.10.2",
    "@types/pako": "^2.0.4",
    "@types/react": "^19.2.7",
    "cac": "^6.7.14",
    "shiki": "^1.24.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.99.0"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@mdxui/beacon": "^0.2.0",
    "@mdxui/cockpit": "^0.2.0",
    "@opentui/core": "^0.1.0",
    "@opentui/react": "^0.1.0",
    "fsx.do": "^0.1.0",
    "hono": "^4.11.3",
    "miniflare": "^3.20241106.0",
    "pako": "^2.1.0",
    "react": "^18.2.0"
  }
}
